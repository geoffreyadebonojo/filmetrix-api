POST http://localhost:3000/graphql
Content-Type: application/json
Authorization: Bearer xxx
X-REQUEST-TYPE: GraphQL

query {
  details(id: 500, entity:"person") {
    id
    alsoKnownAs
    summary
    year
    deathday
    homepage
    imdbId
    name
    knownForDepartment
    placeOfBirth
    popularity
    poster
  }    
}